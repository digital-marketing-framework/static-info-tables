{
    "name": "digital-marketing-framework/typo3-static-info-tables",
    "description": "Provides standard value maps from static_info_tables to the Digital Marketing Framework.",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "marketing",
        "framework",
        "data",
        "relay",
        "static_info_tables",
        "country",
        "iso",
        "code"
    ],
    "authors": [
        {
            "name": "Mediatis AG",
            "email": "info@mediatis.de",
            "homepage": "https://www.mediatis.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://www.mediatis.de",
    "require": {
        "php": "^8.1",
        "digital-marketing-framework/typo3-core": "^1.0",
        "typo3/cms-core": "^11.5 || ^12.4"
    },
    "require-dev": {
        "mediatis/typo3-coding-standards": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalMarketingFramework\\Typo3\\StaticInfoTables\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalMarketingFramework\\Typo3\\StaticInfoTables\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "dmf_static_info_tables",
            "web-dir": ".Build/Web"
        }
    }
}
